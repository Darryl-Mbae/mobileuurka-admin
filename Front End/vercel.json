{
  "name": "mobileuurka-admin-system",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SERVER_URL": "@vite_server_url",
    "VITE_SOCKET_URL": "@vite_socket_url",
    "VITE_RECAPTCHA_SITE_KEY": "@vite_recaptcha_site_key"
  }
}